{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "build-win": "pkg . --targets node18-win-x64 --output ./build/naps2-service",
    "build-linux": "pkg . --targets node18-linux-x64 --output ./build/naps2-service",
    "build-mac": "pkg . --targets node18-macos-x64 --output ./build/naps2-service-macos",
    "pkg": "pkgbuild --root ./build --identifier com.naps2.installer --version 1.0.0 --scripts ./scripts --install-location /usr/local/naps2-service naps2-installer.pkg"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^17.2.1",
    "express": "~4.16.1",
    "nodemon": "^3.1.10",
    "pkg": "^5.8.1"
  }
}
